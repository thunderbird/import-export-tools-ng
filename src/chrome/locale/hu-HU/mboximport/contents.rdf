      <RDF:Seq about="urn:mozilla:locale:hu-HU:packages">
        <RDF:li resource="urn:mozilla:locale:hu-HU:mboximport"/>
      </RDF:Seq>
    </chrome:packages>
  </RDF:Description>

</RDF:RD
