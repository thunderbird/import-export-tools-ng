{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "version": "15.0.0-a5",
  "description": "__MSG_extensionDescription__",
  "author": "Christopher Leidigh",
  "applications": {
    "gecko": {
      "id": "ImportExportToolsNG@cleidigh.kokkini.net",
      "strict_min_version": "140.0",
      "strict_max_version": "145.*"
    }
  },
  "browser_action": {
    "default_icon": "chrome/content/mboximport/icons/import-export-tools-ng-icon-64px.png",
    "default_title": "ImportExport",
    "default_popup": "chrome/content/dummy.html"
  },
  "experiment_apis": {
    "WindowListener": {
      "schema": "api/WindowListener/schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "paths": [
          [
            "WindowListener"
          ]
        ],
        "script": "api/WindowListener/implementation.js"
      }
    },
    "NotifyTools": {
      "schema": "api/NotifyTools/schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "paths": [
          [
            "NotifyTools"
          ]
        ],
        "script": "api/NotifyTools/implementation.js",
        "events": [
          "startup"
        ]
      }
    },
    "AsyncPrompts": {
      "schema": "api/AsyncPrompts/schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "paths": [
          [
            "AsyncPrompts"
          ]
        ],
        "script": "api/AsyncPrompts/implementation.js"
      }
    },
    "ExportMessages": {
      "schema": "api/ExportMessages/schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "paths": [
          [
            "ExportMessages"
          ]
        ],
        "script": "api/ExportMessages/implementation.js"
      }
    },
    "LegacyHelper": {
      "schema": "api/LegacyHelper/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "paths": [["LegacyHelper"]],
        "script": "api/LegacyHelper/implementation.js"
      }
    },
    "LegacyPrefs": {
      "schema": "api/LegacyPrefs/schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "paths": [
          [
            "LegacyPrefs"
          ]
        ],
        "script": "api/LegacyPrefs/implementation.js"
      }
    }
  },
  "permissions": [
    "menus",
    "activeTab",
    "messagesRead",
    "tabs",
    "accountsFolders",
    "accountsRead",
    "messagesImport",
    "messagesMove"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "type": "module"
  },
  "icons": {
    "64": "chrome/content/mboximport/icons/import-export-tools-ng-icon-64px.png"
  },
  "default_locale": "en-US"
}