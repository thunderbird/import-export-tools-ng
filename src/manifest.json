{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "version": "12.0.2-t7",
  "description": "__MSG_extensionDescription__",
  "author": "Christopher Leidigh",
  "applications": {
    "gecko": {
      "id": "ImportExportToolsNG@cleidigh.kokkini.net",
      "strict_min_version": "91.0",
      "strict_max_version": "102.*"
    }
